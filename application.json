{
    "$id":"",
    "$schema":"",


    "title":"application",
    "type": "object",
    "properties":{

        "id":{
            "title":"ID",
            "type":"number"
        },

        "name": {
            "type": "object",
            "properties": {
                "first": {
                    "title": "First Name",
                    "type": "string"
                },
                "middle": {
                    "title": "Middle Name",
                    "type": "string"
                },
                "last": {
                    "title": "Last Name",
                    "type": "string"
                }
            }
        },

        "redirect_url": {
            "type": "string",
            "title": "Redirect URL"
        },

        "allowed_domains": {
            "type": "string",
            "title": "Allowed Domains"
        },

        "organization": {
            "type": "string",
            "title": "Organization"
        },

        "created_at": {
            "type": "string",
            "format": "date-time",
            "title": "Created At"
        },

        "created_by": {
            "type": "string",
            "title": "Created By"
        },

        "client_key": {
            "type": "string",
            "title": "Client Key"
        },

        "client_secret":{
            "type":"string",
            "title":"Client Secret"
        }
    },
    "required":["id"]
    
}