{
    "$id":"",
    "$schema":"",


    "title":"users",
    "type": "object",
    "properties":{

        "id":{
            "title":"ID",
            "type":"number"
        },

        "full_name": {
            "type": "object",
            "properties": {
                "first": {
                    "title": "First Name",
                    "type": "string"
                },
                "middle": {
                    "title": "Middle Name",
                    "type": "string"
                },
                "last": {
                    "title": "Last Name",
                    "type": "string"
                }
            }
        },

        "created_at": {
            "type": "string",
            "format": "date-time",
            "title": "Created At"
        },

        "email": {
            "type": "string",
            "format": "email",
            "title": "E-Mail ID",
            "minLength": 6
        },

        "verified": {
            "type": "boolean",
            "title": "Verified"
        },

        "active": {
            "type": "boolean",
            "title": "Active"
        },

        "password":{
            "type":"string",
            "title":"Password"
        },

        "phone_number": {
            "type": "string",
            "title":"Phone Number"
        },

        "github_id":{
            "type":"string",
            "title":"Github ID"
        },

        "dribble_id":{
            "type":"string",
            "title":"Github ID"
        },

        "img_url":{
            "type":"string",
            "title":"Profile Image URL"
        }
    },
    "required":["id"]
    
}